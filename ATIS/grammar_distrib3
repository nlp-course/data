        | 'are'
        | 'my'
        | 'choices'
        | 'for'
        | 'i'
        | 'would'
        | 'like'
        | "i'd"
        | 'to'
        | 'see'
        | 'have'
        | 'make'
        | 'book'
        | 'find'
        | 'information'
        | 'on'
        | 'know'
        | 'some'
        | 'hello'
        | 'yes'
        | 'please'
        | 'repeat'
        | 'do'
        | 'have'
        | 'there'
        | 'need'
        | 'hi'
        | 'get'
        | 'may'
        | 'listing'
        | 'listings'
        | 'travel'
        | 'arrangements'
        | 'okay'
        | 'want'
        | 'tell'
        | 'about'
        | 'how'
        | 'would'
        | 'be'
        | 'able'
        | 'put'
        | 'requesting'
        | "i'm"
        | 'looking'
        | 'display'

